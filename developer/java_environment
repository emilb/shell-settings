# Maven
MAVEN_OPTS="-Xmx1024M -Xms512M -XX:PermSize=512M -XX:MaxPermSize=1024M"
MAVEN_HOME="~/Applications/apache-maven-3.0.3"
export MAVEN_OPTS MAVEN_HOME

# ANT

# JAVA_HOME
JAVA_HOME="/System/Library/Frameworks/JavaVM.framework/Versions/CurrentJDK/Home"
export JAVA_HOME


export PATH=$PATH:$MAVEN_HOME/bin